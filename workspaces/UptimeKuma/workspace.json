{
  "description": "Open source monitoring tool",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "uptime.jpg",
  "name": "louislam/uptime-kuma:1",
  "run_config": {
    "hostname": "kasm"
  },
  "ports": [
    {
      "3001/tcp": {}
    }
  ],
  "gpu_enabled": false,
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/custom_startup.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "Monitoring"
  ],
  "friendly_name": "Uptime Kuma",
  "architecture": [
    "amd64"
  ],
  "compatibility": [
    "1.15.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 512
}
